      <features
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/total"/>
      <features
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/functional"/>
      <editableFeatures
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/total"/>
      <editableFeatures
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/functional"/>
    </labelMappings>
    <containmentFeature
        href="classdiagrams.ecore#//Classdiagram/associations"/>
    <tool
        xsi:type="gmftool:CreationTool"
        href="classdiagrams.gmftool#//@palette/@tools.2/@tools.0"/>
    <diagramLink
        href="classdiagrams.gmfgraph#Association"/>
    <sourceMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagrams.ecore#//Association/source"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagrams.ecore#//Association/target"/>
  </links>
  <links>
    <tool
        xsi:type="gmftool:CreationTool"
        href="classdiagrams.gmftool#//@palette/@tools.2/@tools.1"/>
    <diagramLink
        href="classdiagrams.gmfgraph#Supertype"/>
    <linkMetaFeature
        xsi:type="ecore:EReference"
        href="classdiagrams.ecore#//Class/supertypes"/>
    <creationConstraints>
      <targetEnd
          body="self &lt;> oppositeEnd and not self->closure(supertypes)->includes(oppositeEnd)"/>
    </creationConstraints>
  </links>
  <diagram>
    <diagramCanvas
        href="classdiagrams.gmfgraph#classdiagrams"/>
    <domainModel
        href="classdiagrams.ecore#/"/>
    <domainMetaElement
        href="classdiagrams.ecore#//Classdiagram"/>
    <palette
        href="classdiagrams.gmftool#//@palette"/>
