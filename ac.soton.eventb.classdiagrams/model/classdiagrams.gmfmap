            href="classdiagrams.ecore#//Class/constraints"/>
        <ownedChild>
          <domainMetaElement
              href="classdiagrams.ecore#//ClassConstraint"/>
          <labelMappings
              xsi:type="gmfmap:FeatureLabelMapping"
              viewPattern="{0}: {1}"
              editorPattern="{0}: {1}">
            <diagramLabel
                href="classdiagrams.gmfgraph#ConstraintLabel"/>
            <features
                href="../../org.eventb.emf.core/model/eventbcore.ecore#//EventBNamed/name"/>
            <features
                href="../../org.eventb.emf.core/model/eventbcore.ecore#//EventBPredicate/predicate"/>
            <editableFeatures
                href="../../org.eventb.emf.core/model/eventbcore.ecore#//EventBNamed/name"/>
            <editableFeatures
                href="../../org.eventb.emf.core/model/eventbcore.ecore#//EventBPredicate/predicate"/>
