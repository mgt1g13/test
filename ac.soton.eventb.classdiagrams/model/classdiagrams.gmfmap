      <editableFeatures
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/surjective"/>
      <editableFeatures
          href="../../ac.soton.eventb.emf.core.extension/model/eventbcoreextension.ecore#//EventBRelationKind/injective"/>
    </labelMappings>
    <labelMappings
