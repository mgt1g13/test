              href="classdiagrams.gmftool#//@palette/@tools.1/@tools.2"/>
          <diagramNode
              href="classdiagrams.gmfgraph#ConstraintLabel"/>
        </ownedChild>
      </children>
      <compartments
          children="//@nodes.0/@ownedChild/@children.0">
        <compartment
            href="classdiagrams.gmfgraph#AttributesCompartment"/>
      </compartments>
      <compartments
          children="//@nodes.0/@ownedChild/@children.1">
